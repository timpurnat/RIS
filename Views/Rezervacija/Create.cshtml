@model web.Models.Rezervacija

@{
    ViewData["Title"] = "Create";
}

<h1>Rezervacija</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="datumPrevzema" class="control-label"></label>
                <input asp-for="datumPrevzema" class="form-control" />
                <span asp-validation-for="datumPrevzema" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="datumZapadlosti" class="control-label"></label>
                <input asp-for="datumZapadlosti" class="form-control" />
                <span asp-validation-for="datumZapadlosti" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IzdelekId" class="control-label"></label>
                <select asp-for="IzdelekId" class="form-control" asp-items="ViewBag.Naslov"></select>
                <span asp-validation-for="IzdelekId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
